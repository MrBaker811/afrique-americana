<div class= "image">
<%= image_tag "DeathtoStock_NotStock.jpg", :width => "100%", :class => "img-responsive" %> 
  <h1_1>Crowns
<br>Over<br>Cotton</h1_1>
       


</div>

<div class="jumbotron">
 <h1>BLOG</h1>
   </div>
   

  <div class="container">
  <%= link_to 'New Blog', new_post_path, class: "btn btn-primary" %>

  
  
 

<div class="jumbotron">

 
<% @posts.each do |post| %>

  
   <em><h2><%= link_to  post.title %></h2></em>
   <td><%=  post.text %></td>
    <p>
     <%= time_ago_in_words post.created_at %> ago

   
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Delete', post_path(post),
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
  
<% end %>
</div.
</div>
